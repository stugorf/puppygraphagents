# Database Configuration
# PostgreSQL connection string for the main application database
DATABASE_URL=postgresql://postgres:postgres123@localhost:5432/temporal_graph

# OpenRouter Configuration (replaces OpenAI)
# Get your API key from https://openrouter.ai/
OPEN_ROUTER_KEY=your_openrouter_api_key_here
# OpenRouter API base URL (defaults to https://openrouter.ai/api/v1)
OPEN_ROUTER_API_BASE=https://openrouter.ai/api/v1

# PuppyGraph Configuration
# PuppyGraph instance credentials and configuration
PUPPYGRAPH_USERNAME=your_puppygraph_username
PUPPYGRAPH_PASSWORD=your_puppygraph_password
# Path to the graph schema configuration file
PUPPYGRAPH_SCHEMA_PATH=./puppygraph-config/graph-schema.json
# PuppyGraph service URL (for Docker setup)
PUPPYGRAPH_URL=http://localhost:8081

# Application Configuration
# Node.js environment (development/production)
NODE_ENV=development
# Port for the application server
PORT=5000
# Session secret for user authentication
SESSION_SECRET=your_session_secret_here

# Development Configuration
# Replit environment (if running on Replit)
REPL_ID=your_repl_id_here

# Legacy OpenAI Configuration (deprecated - use OpenRouter instead)
# OPENAI_API_KEY=your_openai_api_key_here
