# Copy this file to .env and fill in your actual values

# OpenAI API Key for DSPy agents
OPENAI_API_KEY=your_openai_api_key_here

# Session Secret for secure sessions
SESSION_SECRET=your_secure_session_secret_here

# Database Configuration (for Docker)
DATABASE_URL=postgresql://postgres:postgres123@postgres:5432/temporal_graph

# PuppyGraph Configuration (for Docker)
PUPPYGRAPH_URL=http://puppygraph:8081
PUPPYGRAPH_USERNAME=puppygraph
PUPPYGRAPH_PASSWORD=puppygraph123
PUPPYGRAPH_GRAPH_SCHEMA_PATH=/app/puppygraph-config/graph-schema.json

# Application Configuration
NODE_ENV=production
PORT=5000

# PostgreSQL Database Settings (for Docker)
POSTGRES_DB=temporal_graph
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123
PUPPYGRAPH_SCHEMA_PATH=/puppygraph/conf/graph-schema.json
